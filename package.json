{
  "name": "@hyperledger/identus-edge-agent-sdk",
  "version": "3.1.0",
  "description": "Identus SDK - Typescript",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.mjs",
    "index.cjs",
    "build/**/*",
    "README.MD",
    "preinstall.sh",
    "postinstall.sh",
    "patches/*.patch"
  ],
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "scripts": {
    "externals:check": "./externals/run.sh -x check",
    "externals:build": "./externals/run.sh -x build",
    "externals:update": "./externals/run.sh -x update",
    "build": "npm run externals:update && rm -rf build && npm run build:agnostic && npm run types",
    "build:dev": "npm run externals:check && rm -rf build && npm run build:agnostic && npm run types",
    "build:agnostic": "node esbuild.config.mjs",
    "types": "rm -rf build/**/*.d.ts && tsc",
    "test": "npx vitest --config vitest.config.mjs --run tests",
    "coverage": "npm run test -- --coverage",
    "lint": "npx eslint .",
    "docs": "npx typedoc --options typedoc.js",
    "prepare": "npx husky",
    "preinstall": "sh preinstall.sh",
    "postinstall": "sh postinstall.sh"
  },
  "author": "IOHK",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/identus-edge-agent-sdk-ts.git"
  },
  "license": "Apache-2.0",
